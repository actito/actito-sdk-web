{
  "name": "@actito/web-core",
  "version": "5.0.0-canary.1",
  "description": "The core package for the 'actito-web' umbrella package.",
  "author": "Actito <mobile@actito.com> (https://actito.com)",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "scripts": {
    "build": "yarn build:types && yarn build:sources",
    "build:sources": "rollup -c",
    "build:types": "tsc",
    "clean": "rm -rf dist/",
    "dev": "concurrently \"yarn dev:types\" \"yarn dev:sources\"",
    "dev:sources": "rollup -c -w",
    "dev:types": "tsc --watch",
    "lint": "eslint .",
    "lint:types": "attw --pack ."
  },
  "dependencies": {
    "@actito/web-cloud-api": "^5.0.0-canary.1",
    "@actito/web-logger": "^5.0.0-canary.1",
    "tslib": "^2.5.0"
  },
  "files": [
    "dist",
    "!dist/intermediate"
  ],
  "publishConfig": {
    "access": "public"
  }
}

{
  "name": "@actito/web-logger",
  "version": "5.0.0-canary.1",
  "description": "A logger package for use in the Actito Web SDK.",
  "author": "Actito <mobile@actito.com> (https://actito.com)",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "scripts": {
    "attw": "attw --pack .",
    "build": "yarn build:types && yarn build:sources",
    "build:sources": "rollup -c",
    "build:types": "tsc",
    "clean": "rm -rf dist/",
    "dev": "concurrently \"yarn dev:types\" \"yarn dev:sources\"",
    "dev:sources": "rollup -c -w",
    "dev:types": "tsc --watch",
    "lint": "eslint .",
    "lint:staged": "lint-staged"
  },
  "files": [
    "dist",
    "!dist/intermediate"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@actito/library-config": "workspace:^",
    "@arethetypeswrong/cli": "^0.18.2",
    "concurrently": "^9.2.0",
    "eslint": "^9.33.0",
    "rollup": "^4.46.3",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
